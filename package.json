{
  "name": "steganogre",
  "version": "0.2.0",
  "description": "Simple steganography lib for browsers",
  "main": "dist/steganogre.min.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "dev": "./node_modules/.bin/watchify ./src/index.js -v -o ./dist/steganogre.js -t [ babelify --presets es2015 stage-2 ] --standalone steganogre",
    "prod": "./node_modules/.bin/browserify ./src/index.js -v -o ./dist/steganogre.min.js -t [ babelify --presets es2015 stage-2 ] -t [ uglifyify --compres --mangle --verbose ] --standalone steganogre"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/popc0rn/steganogre.git"
  },
  "keywords": [
    "steganography",
    "picture",
    "image",
    "hide",
    "ogre"
  ],
  "author": "popc0rn",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/popc0rn/steganogre/issues"
  },
  "homepage": "https://github.com/popc0rn/steganogre#readme",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "jasmine": "^2.4.1",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "standard": "^8.1.0",
    "watchify": "^3.7.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "beforeEach",
      "spyOn"
    ]
  }
}
